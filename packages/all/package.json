{
  "name": "taro-apollo",
  "description": "Taro Apollo Hooks, Components, and HOC.",
  "version": "0.1.1",
  "author": "opensource@apollographql.com",
  "keywords": [
    "apollo",
    "graphql",
    "taro",
    "hooks",
    "hoc",
    "components"
  ],
  "license": "MIT",
  "main": "./lib/taro-apollo.cjs.js",
  "module": "./lib/taro-apollo.esm.js",
  "react-native": {
    "react-dom/server": false
  },
  "typings": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "burnhamzhang/taro-apollo"
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rm -Rf ./lib/* ./meta/bundlesize/* ./meta/coverage/* ./.rpt2_cache",
    "prepare": "npm run build",
    "prebuild": "npm run clean",
    "build": "npx tsc -p ./config",
    "postbuild": "npx rollup -c ./config/rollup.config.js",
    "predeploy": "npm run build",
    "deploy": "npm publish"
  },
  "peerDependencies": {
    "@tarojs/taro": "^1.3.18",
    "apollo-client": "^2.6.4",
    "graphql": "^14.3.1"
  },
  "dependencies": {
    "@apollo/taro-common": "file:../common",
    "@apollo/taro-components": "file:../components",
    "@apollo/taro-hoc": "file:../hoc",
    "@apollo/taro-hooks": "file:../hooks"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "rollup": "1.20.3",
    "typescript": "3.5.3"
  }
}
